# Agent Service Environment Variables

# Debug mode
DEBUG=true

# Core service URL (all data access through here)
CORE_SERVICE_URL=http://core:8000

# LangGraph checkpoint database (separate from Django database)
LANGGRAPH_DATABASE_URL=postgresql://changple:changple@postgres:5432/changple_langgraph

# Redis
REDIS_URL=redis://redis:6379/1

# AI service keys
OPENAI_API_KEY=sk-...
GOOGLE_API_KEY=...

# Pinecone
PINECONE_API_KEY=...
PINECONE_INDEX_NAME=changple-index
PINECONE_ENVIRONMENT=us-east-1

# LLM settings (optional)
DEFAULT_MODEL=gemini-2.5-flash
EMBEDDING_MODEL=text-embedding-3-large
