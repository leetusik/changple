# =============================================================================
# Changple AI - Secrets Only
# =============================================================================
# Copy to .env and fill in values. NEVER commit .env to version control!
# Non-secret configuration is in config/*.toml files.

# -----------------------------------------------------------------------------
# Database Credentials
# -----------------------------------------------------------------------------
POSTGRES_USER=changple
POSTGRES_PASSWORD=your-secure-password-here

# -----------------------------------------------------------------------------
# Django Secret Key
# Generate: python -c "from django.core.management.utils import get_random_secret_key; print(get_random_secret_key())"
# -----------------------------------------------------------------------------
DJANGO_SECRET_KEY=your-django-secret-key-here

# -----------------------------------------------------------------------------
# AI Service API Keys
# -----------------------------------------------------------------------------
OPENAI_API_KEY=sk-your-openai-key
GOOGLE_API_KEY=your-google-api-key
PINECONE_API_KEY=your-pinecone-api-key

# -----------------------------------------------------------------------------
# Naver OAuth
# -----------------------------------------------------------------------------
SOCIAL_AUTH_NAVER_KEY=your-naver-client-id
SOCIAL_AUTH_NAVER_SECRET=your-naver-client-secret

# -----------------------------------------------------------------------------
# Naver API (Scraper)
# -----------------------------------------------------------------------------
NAVER_CLIENT_ID=your-naver-api-client-id
NAVER_CLIENT_SECRET=your-naver-api-client-secret
